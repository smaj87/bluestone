  "scripts": {
    "build": "cross-env APP=bluestone NODE_ENV=production IS_PREACT=TRUE webpack --config webpack/webpack.prod.babel.mjs --color --progress",
    "check-types": "tsc --noEmit",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint app/**/*.{js,jsx,ts,tsx}",
    "lint:eslint": "eslint --ignore-path .eslintignore",
    "lint:eslint:fix": "eslint --ignore-path .eslintignore . --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "presetup": "npm i chalk shelljs",
    "prestart:prod": "cross-env APP=bluestone NODE_ENV=production IS_START_LOCAL_PROD=true npm run build",
    "prettify": "prettier --write",
    "start": "cross-env APP=bluestone NODE_ENV=development IS_PREACT=TRUE node server/index.mjs",
    "start:prod": "cross-env APP=bluestone NODE_ENV=production IS_START_LOCAL_PROD= IS_PREACT=TRUE node server/index.mjs"
  },
  "pre-commit": "lint:staged",
